{
  "name": "kws2100-exam-amaliye",
  "version": "1.0.0",
  "description": "[![Review Assignment Due Date](https://classroom.github.com/assets/deadline-readme-button-22041afd0340ce965d47ae6ef1cefeee28c7c493a6346c4f15d667ab976d596c.svg)](https://classroom.github.com/a/XamnFLGl)",
  "main": "index.js",
  "scripts": {
    "test": "tsc --noEmit && prettier --check .",
    "dev": "vite",
    "build": "vite build",
    "prepare": "husky",
    "postinstall": "cd server && npm install --include=dev",
    "start": "cd server && npm start",
    "db:culturalheritage": "npm run db:culturalheritage:download && npm run db:culturalheritage:import",
    "db:culturalheritage:download": "download --extract --out tmp/ https://nedlasting.geonorge.no/geonorge/Kulturminner/Kulturminner/PostGIS/Kulturminner_0000_Norge_25833_Kulturminner_PostGIS.zip",
    "db:culturalheritage:import": "docker exec -i /postgis /usr/bin/psql --user postgres < tmp/Kulturminner_0000_Norge_25833_Kulturminner_PostGIS.sql",
    "db:heroku": "npm run db:heroku:postgis && npm run db:heroku:data && npm run db:heroku:prepare",
    "db:heroku:data": "npm run db:culturalheritage:download && psql $DATABASE_URL < tmp/Kulturminner_0000_Norge_25833_Kulturminner_PostGIS.sql",
    "db:heroku:prepare": "psql $DATABASE_URL < scripts/prepare-db.sql"
  },
  "keywords": [],
