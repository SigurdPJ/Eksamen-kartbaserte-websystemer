    "@types/node": "^22.15.12",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@types/proj4": "^2.5.6",
    "download-cli": "^1.0.1",
    "ol": "^10.5.0",
    "proj4": "^2.15.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
