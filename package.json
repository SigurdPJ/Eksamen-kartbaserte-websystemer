{
  "name": "kws2100-exam-amaliye",
  "version": "1.0.0",
  "description": "[![Review Assignment Due Date](https://classroom.github.com/assets/deadline-readme-button-22041afd0340ce965d47ae6ef1cefeee28c7c493a6346c4f15d667ab976d596c.svg)](https://classroom.github.com/a/XamnFLGl)",
  "main": "index.js",
  "scripts": {
    "test": "tsc --noEmit && prettier --check .",
    "dev": "vite",
    "build": "vite build",
    "prepare": "husky",
    "postinstall": "cd server && npm install --include=dev",
    "start": "cd server && npm start",
    "db:railways": "npm run db:railways:download && npm run db:railways:import",
    "db:railways:download": "download --extract --out tmp/ https://nedlasting.geonorge.no/geonorge/Samferdsel/Banenettverk/PostGIS/Samferdsel_0000_Norge_25833_Banenettverk_PostGIS.zip",
    "db:railways:import": "docker exec -i /postgis /usr/bin/psql --user postgres < tmp/Samferdsel_0000_Norge_25833_Banenettverk_PostGIS.sql",
    "db:heroku:postgis": "echo 'create extension postgis' | psql \"$DATABASE_URL\"",
    "db:railways:heroku": "npm run db:railways:download && psql \"$DATABASE_URL\" < tmp/Samferdsel_0000_Norge_25833_Banenettverk_PostGIS.sql",
    "db:heroku": "npm run db:heroku:postgis && npm run db:railways:heroku"
  },
